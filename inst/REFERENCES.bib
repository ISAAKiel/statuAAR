@article{olivier_estimation_1975,
	title = {Estimation de la stature féminine d'après les os longs des membres},
	volume = {2},
	rights = {free},
	url = {https://www.persee.fr/doc/bmsap_0037-8984_1975_num_2_4_1821},
	doi = {10.3406/bmsap.1975.1821},
	abstract = {Conclusions et résumé. Alors que l'estimation de la stature masculine est possible à l'aide des ossements de soldats dont la taille était connue de leur vivant, l'estimation de la stature féminine n'était possible que par les données cadavériques où la stature est toujours mal mesurée ; aussi nous proposons le procédé indirect suivant. Nous avons utilisé les équivalences établies entre mensurations externes des membres et les longueurs des os longs (Olivier et Pineau, 1957) ; cela nous a permis d'utiliser les données de S. de Félice (1958) sur les femmes françaises en remplaçant les segments de membres par les longueurs équivalentes des os. Nous avons ainsi calculé les valeurs les plus probables de la stature pour chaque dimension osseuse. Par rapport aux formules de Breitinger (1937) et de Trotter et Gleser (1958), nous obtenons des estimations plus faibles. Par rapport à celle de Manouvrier (1893) et à la série de Rollet que nous avons complétée, nous trouvons des valeurs plus fortes. Nous sommes satisfaits de nous trouver dans le juste milieu et nous pensons que, dorénavant, pour estimer la stature d'un squelette féminin, il vaudra mieux utiliser les formules indiquées dans le texte.},
	pages = {297--305},
	number = {4},
	journaltitle = {Bulletins et Mémoires de la Société d'Anthropologie de Paris},
	author = {Olivier, Georges and Tissier, Henri},
	urldate = {2020-12-27},
	date = {1975},
