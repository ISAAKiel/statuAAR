@article{mahler_formalized_2022,
	title = {A formalized approach to choosing the best methods for reconstructing stature in the case of poorly preserved skeletal series},
	volume = {64},
	issn = {1475-4754},
	url = {https://onlinelibrary.wiley.com/doi/abs/10.1111/arcm.12700},
	doi = {10.1111/arcm.12700},
	abstract = {Stature, as the universal measure of the well-being of a population, is one of the three basic characteristics, besides sex and age, of every osteological analysis. Reconstructing living stature using the length of long bones and linear regression is the method of choice, especially in the case of poorly preserved human skeletal material obtained from archaeological excavations. Although the choice of the best formulae is usually based on intuition, the need for a more formal approach is paramount. However, the solutions adopted to date require a significant number of well-preserved skeletons, which is rarely possible in bioarchaeological research. For skeletal series consisting of very incomplete skeletons, it has been shown that a simple comparison of the mean differences in stature estimation using femur, humerus, and radius bone pairs may be instrumental for selecting the most appropriate of the available stature reconstruction methods. The results were confronted with the consistency of the same methods but also using bones that could not have been used previously; that is the tibia, fibula and ulna. All the methods tested were then compared with those selected as being the most consistent to see if the results they produce are significantly different.},
	pages = {265--280},
	number = {1},
	journaltitle = {Archaeometry},
	author = {Mahler, Robert},
	urldate = {2022-04-11},
	date = {2022},
	langid = {english},
	note = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1111/arcm.12700},
	file = {Mahler_2022_A formalized approach to choosing the best methods for.pdf:C\:\\Users\\suguf260\\biblio\\storage\\Mahler2022\\Mahler_2022_A formalized approach to choosing the best methods for.pdf:application/pdf;Snapshot:C\:\\Users\\suguf260\\Zotero\\storage\\X7BKHZ59\\arcm.html:text/html},
}

@article{byers_determination_1989,
	title = {Determination of adult stature from metatarsal length},
	volume = {79},
	rights = {Copyright © 1989 Wiley-Liss, Inc., A Wiley Company},
	issn = {1096-8644},
	url = {https://onlinelibrary.wiley.com/doi/abs/10.1002/ajpa.1330790303},
	doi = {https://doi.org/10.1002/ajpa.1330790303},
	abstract = {The results of a study to determine the value of foot bones in reconstructing stature are presented. The data consist of length measurements taken on all ten metatarsals as well as on cadaver length from a sample of 130 adults of documented race, sex, stature, and, in most cases, age. Significant correlation coefficients (.58–.89) are shown between known stature and foot bone lengths. Simple and multiple regression equations computed from the length of each of these bones result in standard errors of estimated stature ranging from 40–76 mm. These errors are larger than those for stature calculated from complete long bones, but are approximately the same magnitude for stature calculated from metacarpals and fragmentary long bones. Given that metatarsals are more likely to be preserved unbroken than are long bones and given the ease with which they are accurately measured, the formulae presented here should prove useful in the study of historic and even prehistoric populations.},
	pages = {275--279},
	number = {3},
	journaltitle = {American Journal of Physical Anthropology},
	author = {Byers, Steve and Akoshima, Kaoru and Curran, Bryan},
	urldate = {2021-01-04},
	date = {1989},
	langid = {english},
	note = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1002/ajpa.1330790303},
	file = {Byers et al_1989_Determination of adult stature from metatarsal length.pdf:C\:\\Users\\suguf260\\biblio\\storage\\Byers et al1989\\Byers et al_1989_Determination of adult stature from metatarsal length.pdf:application/pdf},
}

@article{groote_body_2011,
	title = {Body mass and stature estimation based on the first metatarsal in humans},
	volume = {144},
	rights = {Copyright © 2011 Wiley-Liss, Inc.},
	issn = {1096-8644},
	url = {https://onlinelibrary.wiley.com/doi/abs/10.1002/ajpa.21458},
	doi = {https://doi.org/10.1002/ajpa.21458},
	abstract = {Archaeological assemblages often lack the complete long bones needed to estimate stature and body mass. The most accurate estimates of body mass and stature are produced using femoral head diameter and femur length. Foot bones including the first metatarsal preserve relatively well in a range of archaeological contexts. In this article we present regression equations using the first metatarsal to estimate femoral head diameter, femoral length, and body mass in a diverse human sample. The skeletal sample comprised 87 individuals (Andamanese, Australasians, Africans, Native Americans, and British). Results show that all first metatarsal measurements correlate moderately to highly (r = 0.62–0.91) with femoral head diameter and length. The proximal articular dorsoplantar diameter is the best single measurement to predict both femoral dimensions. Percent standard errors of the estimate are below 5\%. Equations using two metatarsal measurements show a small increase in accuracy. Direct estimations of body mass (calculated from measured femoral head diameter using previously published equations) have an error of just over 7\%. No direct stature estimation equations were derived due to the varied linear body proportions represented in the sample. The equations were tested on a sample of 35 individuals from Christ Church Spitalfields. Percentage differences in estimated and measured femoral head diameter and length were less than 1\%. This study demonstrates that it is feasible to use the first metatarsal in the estimation of body mass and stature. The equations presented here are particularly useful for assemblages where the long bones are either missing or fragmented, and enable estimation of these fundamental population parameters in poorly preserved assemblages. Am J Phys Anthropol, 2011. © 2011 Wiley-Liss, Inc.},
	pages = {625--632},
	number = {4},
	journaltitle = {American Journal of Physical Anthropology},
	author = {Groote, Isabelle De and Humphrey, Louise T.},
	urldate = {2021-01-04},
	date = {2011},
	langid = {english},
	note = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1002/ajpa.21458},
	file = {Groote_Humphrey_2011_Body mass and stature estimation based on the first.pdf:C\:\\Users\\suguf260\\biblio\\storage\\Groote_Humphrey2011\\Groote_Humphrey_2011_Body mass and stature estimation based on the first.pdf:application/pdf},
}

